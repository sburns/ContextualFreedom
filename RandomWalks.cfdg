startshape start
background { b -1 }

rule start {
	CIRCLE  { s .5 b -1 z 1 }
	walk { sat .5 b 1 |h 359 h 1 }
}
 
rule walk { CIRCLE { s .008 } walk { x 1 h .000001|} }
rule walk { CIRCLE { s .012 } walk { x -1 h .000001| } }
rule walk { CIRCLE { s .015 } walk { y 1 h .000001| } }
rule walk { CIRCLE { s . 02 } walk { y -1 h .000001| } }

rule walk 0.00001 { CIRCLE  { s 1 b -1 } }

